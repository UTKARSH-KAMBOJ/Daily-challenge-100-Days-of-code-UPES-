
/*Q78: Find the sum of main diagonal elements for a square matrix.

/*
Sample Test Cases:
Input 1:
3 3
1 2 3
4 5 6
7 8 9
Output 1:
15

*/

#include <stdio.h>

int main() {
    int n, m;
    int sum = 0;
    int i, j;

    printf("Enter the row limit: ");
    scanf("%d", &n);
    printf("Enter the column limit: ");
    scanf("%d", &m);

    int arr[n][m];

    for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            printf("arr[%d][%d]: ", i, j);
            scanf("%d", &arr[i][j]);
        }
    }

    // Sum of main diagonal elements
    for (i = 0; i < n && i < m; i++) {
        sum += arr[i][i];
    }

    printf("%d\n", sum);

    return 0;
}



