/*Q76: Check if a matrix is symmetric.

/*
Sample Test Cases:
Input 1:
2 2
1 2
2 1
Output 1:
True

Input 2:
2 2
1 0
2 1
Output 2:
False

*/
#include <stdio.h>

int main() {
    int n, m;
    printf("Enter the number of rows taken only 2: ");
    scanf("%d", &n);
    
    printf("Enter the number of columns taken only 2: ");
    scanf("%d", &m);
    
    int arr[n][m];
    int i, j;

    printf("Enter the elements of the matrix:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            printf("arr[%d][%d]",i,j);
            scanf("%d", &arr[i][j]);
        }
    }

    printf("The matrix is:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < m; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n"); 
    }
   if(arr[0][1]==arr[1][0]){
    printf("symmetric matrix");

   }
   else{
    printf("not symmetric matrix ");
   }
    

    return 0;
}


