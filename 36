/*Q72: Find the sum of all elements in a matrix.

/*
Sample Test Cases:
Input 1:
2 3
1 2 3
4 5 6
Output 1:
21

*/
#include<stdio.h>
int main(){
    int i=0,j=0;
    int n=0;
    int m=0;
    int sum=0;
    printf("enter the limit of row:");
    scanf("%d",&n);
    printf("enter the limit of column:");
    scanf("%d",&m);
    int arr[n][m];
    printf("enter the elements:");
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){printf("element [%d][%d]:",i+1,j+1);
            scanf("%d",&arr[i][j]);
        }}
        for(i=0;i<n;i++){
        for(j=0;j<m;j++){
             sum+=arr[i][j];
        }}
        printf("%d",sum);
    
    
    return 0;

    }
    
