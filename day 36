/*Q71: Read and print a matrix.

/*
Sample Test Cases:
Input 1:
2 2
1 2
3 4
Output 1:
1 2
3 4

*/
#include<stdio.h>
int main(){
    int i=0,j=0;
    int n=0;
    int m=0;
    printf("enter the limit of row:");
    scanf("%d",&n);
    printf("enter the limit of column:");
    scanf("%d",&m);
    int arr[n][m];
    printf("enter the elements:");
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&arr[i][j]);
        }
    }
     printf("the elements:\n");
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            printf("%d",arr[i][j]);
        }
        printf("\n");
    }
    return 0;
}
