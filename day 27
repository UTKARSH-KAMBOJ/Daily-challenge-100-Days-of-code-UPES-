/*Q53: Write a program to print the following pattern:
*
***
*****
*******
*********
*******
*****
***
*

/*
Sample Test Cases:
Input 1:

Output 1:
*
***
*****
*******
*********
*******
*****
***
*

*/
#include<stdio.h>
int main(){
    int n=9;
    int i;
   for(i=1;i<=n;i++){
    if(i==1){
        printf("*");
        printf("\n\n");
    }
    else if(i==2){
        printf("***");
        printf("\n\n");

    }
    else if(i==3){
        printf("*****");
        printf("\n\n");

    }
    else if(i==4){
        printf("*******");
        printf("\n\n");
    }
    else if(i==5){
        printf("*********");
        printf("\n\n");

    }
    else if(i==6){
        printf("*******");
        printf("\n\n");
    
}
else if(i==7){
        printf("*****");
        printf("\n\n");

}
  else if(i==8){
        printf("***");
        printf("\n\n");}
   else if(i==9){
        printf("*");
    

    }
}
return 0;

}
